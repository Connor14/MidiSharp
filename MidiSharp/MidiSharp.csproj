<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.9.0</Version>
    <Description>C# library for reading, writing, and manipulating MIDI (*.mid) files. 

This package is a .NET Standard fork of the original repository: https://github.com/stephentoub/MidiSharp</Description>
    <Copyright>Copyright (c) 2014 Stephen Toub</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/Connor14/MidiSharp</PackageProjectUrl>
    <PackageTags>music, midi</PackageTags>
    <PackageReleaseNotes>Initial .NET Standard 2.0 release.</PackageReleaseNotes>
    <Authors>Stephen Toub</Authors>
    <PackageId>MidiSharp.NetStandard</PackageId>
  </PropertyGroup>

</Project>